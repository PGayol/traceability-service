<div class="event">
  <h4 style="font-weight: 300;">
    <span class="tooltipped" data-position="bottom" data-tooltip="Commit Comment">{{model.commitComment}}</span>
  </h4>
  <div class="row">
  <div class="col s6">
    <dl>
      <dt>Branch</dt>
      <dd class="valign-wrapper"><i class="small material-icons">label</i>{{model.branch.branchPath}}</dd>
      <dt>Commit Date</dt>
      <dd class="valign-wrapper"><i class="small material-icons">schedule</i>{{model.commitDate}}</dd>
      <dt>Username</dt>
      <dd class="valign-wrapper"><i class="small material-icons">perm_identity</i>{{model.user.username}}</dd>
    </dl>
  </div>
  <div class="col s6">
    <dl>
      <dt>Activity Type</dt>
      <dd class="valign-wrapper"><i class="small material-icons">assignment</i>{{model.activityType}}</dd>
      <dt>Highest Currently Promoted Branch</dt>
      <dd class="valign-wrapper"><i class="small material-icons">trending_up</i>{{model.highestPromotedBranch.branchPath}}</dd>
    </dl>
  </div>
  </div>
  <div class="divider"></div>
  {{#each model.conceptChanges as |change|}}
    <p>Concept: {{change.conceptId}}</p>
  {{/each}}
</div>
<script>
  $('.tooltipped').tooltip({delay: 50});
</script>